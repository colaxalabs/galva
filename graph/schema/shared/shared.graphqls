# GraphQL schema
scalar Time

type Land {
  id: ID!
  postalCode: String!
  location: String!
  state: String!
  sateliteImage: String!
  createdAt: Time!
  updatedAt: Time!
  userAddress: String!
  user: User!
}

type User {
  id: ID!
  address: String!
  signature: String!
  createdAt: Time!
  updatedAt: Time!
  properties: [Land!]!
}

