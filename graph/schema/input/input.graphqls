# GraphQL schema
input RegisterUser {
  address: String!
  signature: String!
}

input PropertyInput {
  id: ID!
  postalCode: String!
  location: String!
  sateliteImage: String!
}

input OfferInput {
  purpose: String!
  size: String!
  duration: Time!
  cost: String!
  propertyId: Int!
  userAddress: String!
}

input AcceptOfferInput {
  id: ID!
  userAddress: String!
}
